-- Scripts:

local function FGUS_fake_script() -- grdnt.LocalScript 
	local script = Instance.new('LocalScript', grdnt)

	local s = script.Parent.UIGradient
	
	while wait(.1) do
		s.Rotation = s.Rotation+2
	end
end
coroutine.wrap(FGUS_fake_script)()
local function LMSNG_fake_script() -- Pattern.LocalScript 
	local script = Instance.new('LocalScript', Pattern)

	local s = script.Parent.UIGradient
	
	while wait(.1) do
		s.Rotation = s.Rotation+2
	end
end
coroutine.wrap(LMSNG_fake_script)()
local function FOFZ_fake_script() -- SetKey.LocalScript 
	local script = Instance.new('LocalScript', SetKey)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(255, 41, 73)}):Play()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {TextColor3 = Color3.fromRGB(37,37,37)}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(37, 37, 37)}):Play()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {TextColor3 = Color3.fromRGB(255, 41, 73)}):Play()
	end)
end
coroutine.wrap(FOFZ_fake_script)()
local function PCXVR_fake_script() -- SetKey.LocalScript 
	local script = Instance.new('LocalScript', SetKey)

	local LocalP = game.Players.LocalPlayer
	local Mouse = LocalP:GetMouse()
	
	prev = LocalP.Character:WaitForChild('HumanoidRootPart').CFrame
	
	script.Parent.MouseButton1Click:connect(function()
		game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(KeyPressed)
			if KeyPressed == ""..script.Parent.Parent.Text.."" then
				prev = LocalP.Character:WaitForChild('HumanoidRootPart').CFrame
				LocalP.Character.Parent = workspace.Terrain
				LocalP.Character.Head:Destroy()
				game:GetService('Workspace'):WaitForChild(LocalP.Name) 
				for i=1,10 do
					LocalP.Character:WaitForChild('HumanoidRootPart').CFrame = prev
					wait()
				end
				
			end
		end)
	end)
end
coroutine.wrap(PCXVR_fake_script)()
local function WPLKH_fake_script() -- ResetKey.LocalScript 
	local script = Instance.new('LocalScript', ResetKey)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(255, 41, 73)}):Play()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {TextColor3 = Color3.fromRGB(37,37,37)}):Play()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {PlaceholderColor3 = Color3.fromRGB(37,37,37)}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(37, 37, 37)}):Play()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {TextColor3 = Color3.fromRGB(255, 41, 73)}):Play()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {PlaceholderColor3 = Color3.fromRGB(255, 41, 73)}):Play()
	end)
end
coroutine.wrap(WPLKH_fake_script)()
local function KIORV_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local debounce = false
	local button = script.Parent
	local bg = script.Parent.Parent 
	local cam = workspace.CurrentCamera
	
	local function toggle()
		if debounce == true then
	
			debounce = false
	
			button:TweenPosition(UDim2.new(0, 0,0.111, 0), "Out", "Sine", 0.2, true)
			game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(255, 41, 73)}):Play()
			game:GetService("TweenService"):Create(bg, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(37, 37, 37)}):Play()
			game:GetService("TweenService"):Create(cam, TweenInfo.new(0.3), {FieldOfView = 70}):Play()
			pcall(function()
	
			end)
	
		elseif debounce == false then
	
			debounce = true
	
			button:TweenPosition(UDim2.new(0.681, 0,0.111, 0), "Out", "Sine", 0.2, true)
			game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(37, 37, 37)}):Play()
			game:GetService("TweenService"):Create(bg, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(255, 41, 73)}):Play()
			game:GetService("TweenService"):Create(cam, TweenInfo.new(0.3), {FieldOfView = 85}):Play()
			pcall(function()
	
			end)
	
		end
	end
	
	script.Parent.MouseButton1Click:Connect(toggle)
end
coroutine.wrap(KIORV_fake_script)()
